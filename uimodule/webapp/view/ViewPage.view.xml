                        <Column width="10em" id="col45" app:property="TipoGestione"><Text text="{i18n>TipoGestione}"/></Column>
                        <Column width="10em" id="col47" app:property="TipoGestione1"><Text text="{i18n>TipoGestione1}"/></Column>
                        <Column width="10em" id="col49" app:property="TipoGestione2"><Text text="{i18n>TipoGestione2}"/></Column>
                        <Column width="10em" id="col51" app:property="Cdl" visible="true"><Text text="{i18n>Cdl}"/></Column>
                        <Column width="10em" id="col53" app:property="Lstar"><Text text="{i18n>Lstar}"/></Column>
                        <Column width="10em" id="col55" app:property="Num"><Text text="{i18n>Num}"/></Column>
                        <Column width="10em" id="col57" app:property="Hper"><Text text="{i18n>Hper}"/></Column>
                        <Column width="10em" id="col59" app:property="Persone" visible="false"><Text text="{i18n>Persone}"/></Column>
                        <Column width="10em" id="col61" app:property="Steus"><Text text="{i18n>Steus}"/></Column>
                        <Column width="10em" id="col63" app:property="Toth"><Text text="{i18n>Toth}"/></Column>
