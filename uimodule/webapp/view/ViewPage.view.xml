<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:l="sap.ui.layout" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:ui="sap.ui.table" xmlns:f="sap.f" controllerName="PM030.APP4.controller.ViewPage">
    <f:DynamicPage id="dynamicPageId" preserveHeaderStateOnScroll="true" showFooter="false" fitContent="true" class="sapUiContentPadding">
        <f:landmarkInfo>
            <f:DynamicPageAccessibleLandmarkInfo rootRole="Region" contentRole="Main" headerRole="Banner" rootLabel="{i18n>Overview_rootLabel}" contentLabel="{i18n>Overview_contentLabel}" headerLabel="{i18n>Overview_headerLabel}"/>
        </f:landmarkInfo>
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>TitoloOdm}" wrapping="true" class="titleText"/>
                </f:heading>
                <f:navigationActions>
                    <!--<Link href="https://www.w3.org/WAI/eval/report-tool/" target="_blank" text="{i18n>accessibility}" />-->
                    <Button text="{i18n>accessibility}" press="accesibilityLink"/>
                    <Button type="Default" text="{i18n>ListVar}" icon="sap-icon://activity-items" press="onListVariant" tooltip="{i18n>List}" ariaLabelledBy="ActionListVariant" ariaDescribedBy="ButtonVariant"/>
                      <Button type="Default" text="{i18n>SalvaVar}" icon="sap-icon://save" press="onPressVariant" tooltip="{i18n>SaveVar}" ariaLabelledBy="ActionSaveVariant" ariaDescribedBy="ButtonSave"/>
                      
                    <Button icon="sap-icon://action-settings" press="onPersoButtonPressed" class="sapUiTinyMarginBegin" tooltip="setting" ariaLabelledBy="setting" ariaDescribedBy="settingAction"/>
                    <Button icon="sap-icon://download" press="onDataExport" class="sapUiTinyMarginBegin" tooltip="download" ariaLabelledBy="download" ariaDescribedBy="downloadAction"/>
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <fb:FilterBar id="filterbarPiste" useToolbar="false" search="onSearchResult" showRestoreButton="true" showClearButton="true">
                        <fb:filterGroupItems>

                            <fb:FilterGroupItem id="dataDiRiferimento" visible="true" groupName="__$INTERNAL$" name="ddr" label="{i18n>DataDiRiferimento}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <DatePicker id="DateP1" required="true" placeholder="Inserisci giorno" displayFormat="dd-MM-YYYY" dateValue="{sFilter>/DataDiRiferimento}"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="periodoDiSelezioneDa" visible="true" groupName="__$INTERNAL$" name="pdsd" label="{i18n>PeriodoDiSelezioneDa}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <DatePicker id="DateP2" required="true" placeholder="da" displayFormat="dd-MM-YYYY" dateValue="{sFilter>/PeriodoDiSelezioneDa}"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="periodoDiSelezioneA" visible="true" groupName="__$INTERNAL$" name="pdsa" label="{i18n>PeriodoDiSelezioneA}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <DatePicker id="DateP3" required="true" placeholder="a" displayFormat="dd-MM-YYYY" dateValue="{sFilter>/PeriodoDiSelezioneA}"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="divisione" visible="true" groupName="__$INTERNAL$" name="div" label="{i18n>Divisione}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>DIVISIONE}" showSecondaryValues="true" items="{ path: 'sHelp>/DIVISIONE', length:2500 }" selectedKeys="{sFilter>/Divisione}">
                                        <core:ListItem key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}" additionalText="{sHelp>Fieldname2}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="index" visible="true" groupName="__$INTERNAL$" name="ind" label="{i18n>Index}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput  id="inIndex" type="Number" showValueHelp="false" maxTokens="100" >
                                      <!--<tokens>
                                        <Token key="{sFilter>key}" text="{sFilter>text}" />
                                      </tokens>-->
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="tipoSchedulazione" visible="true" groupName="__$INTERNAL$" name="ts" label="{i18n>TipoSchedulazione}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Select id="tipoFrequenza" selectedKey="{sFilter>/TipoSchedulazione}">
                                        <items>
                                            <core:ListItem xmlns:core="sap.ui.core" key="" text=""/>
                                            <core:ListItem xmlns:core="sap.ui.core" key="C" text="C"/>
                                            <core:ListItem xmlns:core="sap.ui.core" key="T" text="T"/>
                                        </items>
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="tipoOrdine" visible="true" groupName="__$INTERNAL$" name="to" label="{i18n>TipoOrdine}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>TIPO_ORDINE}" showSecondaryValues="true" items="{ path: 'sHelp>/TIPO_ORDINE' }" selectedKeys="{sFilter>/TipoOrdine}">
                                        <core:ListItem key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}" additionalText="{sHelp>Fieldname2} - {sHelp>Fieldname3}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="tipoAttivita" visible="true" groupName="__$INTERNAL$" name="ta" label="{i18n>TipoAttivita}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>TIPO_ATTIVITA}" showSecondaryValues="true" items="{ path: 'sHelp>/TIPO_ATTIVITA', length:2000 }" selectedKeys="{sFilter>/TipoAttivita}">
                                        <core:ListItem key="{sHelp>Fieldname1}" text="{sHelp>Fieldname1}" additionalText="{sHelp>Fieldname2}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="indisponibilita" visible="true" groupName="__$INTERNAL$" name="indisp" label="{i18n>Indisponibilita}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Select id="onlyActive" selectedKey="{sFilter>/Indisponibilita}">
                                        <items>
                                            <core:ListItem xmlns:core="sap.ui.core" key="" text=""/>
                                            <core:ListItem xmlns:core="sap.ui.core" key="0" icon="sap-icon://decline" text="0"/>
                                            <core:ListItem xmlns:core="sap.ui.core" key="1" icon="sap-icon://accept" text="1"/>
                                        </items>
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="sistema" visible="true" groupName="__$INTERNAL$" name="sis" label="{i18n>Sistema}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox change="onFilterHelp" valueStateText="{i18n>valueState} {i18n>SISTEMA}" showSecondaryValues="true" items="{ path: 'sHelp>/SISTEMA', length:2000 }" selectedKeys="{sFilter>/Sistema}">
                                        <core:ListItem key="{sHelp>Sistema}" text="{sHelp>Sistema}" additionalText="{sHelp>Txt}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="azione" visible="true" groupName="__$INTERNAL$" name="azione" label="{i18n>Azione}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput id="inAzione" showSuggestion="true" maxSuggestionWidth="40em" maxTokens="100" showValueHelp="false" value="{sFilter>/Azione}"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="classe" visible="true" groupName="__$INTERNAL$" name="classe" label="{i18n>Classe}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>CLASSE}" showSecondaryValues="true" items="{ path: 'sHelp>/CLASSE', length:2000 }" selectedKeys="{sFilter>/Classe}">
                                        <core:ListItem key="{sHelp>Classe}" text="{sHelp>Classe}" additionalText="{sHelp>Txt}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="impianto" visible="true" groupName="__$INTERNAL$" name="impianto" label="{i18n>Impianto}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput id="inImpianto" showSuggestion="true" maxSuggestionWidth="40em" maxTokens="100" showValueHelp="false" value="{sFilter>/Impianto}"/>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem id="FIIStComponente" visible="true" groupName="__$INTERNAL$" name="FYIStComponente" label="{i18n>SedeTecnicaComponente}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput id="inSede" showSuggestion="true" maxSuggestionWidth="40em" maxTokens="100" showValueHelp="false" value="{sFilter>/SedeTecnicaComponente}" suggestionRows="{sHelp>/SedeRealeSingle}" suggest=".onSuggestSEDE">
                                        <suggestionColumns>
                                            <Column>
                                                <Label text="{i18n>Functlocation}"/>
                                            </Column>
                                            <Column hAlign="Center">
                                                <Label text="{i18n>Category}"/>
                                            </Column>
                                            <Column hAlign="End">
                                                <Label text="{i18n>Descript}"/>
                                            </Column>
                                        </suggestionColumns>
                                        <suggestionRows>
                                            <ColumnListItem>
                                                <Label text="{sHelp>Functlocation}"/>
                                                <Label text="{sHelp>Category}"/>
                                                <Label text="{sHelp>Descript}"/>
                                            </ColumnListItem>
                                        </suggestionRows>
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem id="FIEquipment" visible="true" groupName="__$INTERNAL$" name="FYEquipment" label="{i18n>ComponenteEquipment}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput id="inEquip" showSuggestion="true" maxSuggestionWidth="40em" maxTokens="100" showValueHelp="false" value="{sFilter>/ComponenteEquipment}" suggestionRows="{sHelp>/EQUIPMENT}" suggest=".onSuggestEquipment">
                                        <suggestionColumns>
                                            <Column>
                                                <Label text="{i18n>Equipment}"/>
                                            </Column>
                                            <Column hAlign="End">
                                                <Label text="{i18n>Description}"/>
                                            </Column>
                                        </suggestionColumns>
                                        <suggestionRows>
                                            <ColumnListItem>
                                                <Label text="{sHelp>Fieldname1}"/>
                                                <Label text="{sHelp>Fieldname3}"/>
                                            </ColumnListItem>
                                        </suggestionRows>
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="descrizioneComponente" visible="true" groupName="__$INTERNAL$" name="dc" label="{i18n>DescrizioneComponente}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input type="Text" value="{sFilter>/DescrizioneComponente}" maxLength="80"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="centroDiLavoro" visible="true" groupName="__$INTERNAL$" name="cdl" label="{i18n>CentroDiLavoro}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>CENTRO_LAVORO}" showSecondaryValues="true" items="{ path: 'sHelp>/CENTRO_LAVORO', length:2500 }" selectedKeys="{sFilter>/CentroDiLavoro}">
                                        <core:ListItem key="{sHelp>Arbpl}" text="{sHelp>Arbpl}" additionalText="{sHelp>Werks} - {sHelp>Destinatario}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="destinatario" visible="true" groupName="__$INTERNAL$" name="dest" label="{i18n>Destinatario}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox valueStateText="{i18n>valueState} {i18n>Destinatario}" showSecondaryValues="true" items="{ path: 'sHelp>/CENTRO_LAVORO', length:2500 }" selectedKeys="{sFilter>/Destinatario}">
                                        <core:ListItem key="{sHelp>Destinatario}" text="{sHelp>Destinatario}" additionalText="{sHelp>Werks} - {sHelp>Arbpl}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="percorso" visible="true" groupName="__$INTERNAL$" name="perc" label="{i18n>Percorso}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input type="Text" value="{sFilter>/Percorso}" maxLength="15"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="tipoGestione" visible="true" groupName="__$INTERNAL$" name="tg" label="{i18n>TipoGestione}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox showSecondaryValues="true" items="{ path: 'sHelp>/TIPO_GESTIONE', length:2000 }" selectedKeys="{sFilter>/TipoGestione}">
                                        <core:ListItem key="{sHelp>TipoGestione}" text="{sHelp>TipoGestione}" additionalText="{sHelp>Divisione} - {sHelp>DesTipoGest}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="finalita" visible="true" groupName="__$INTERNAL$" name="fin" label="{i18n>Finalita}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox showSecondaryValues="true" items="{ path: 'sHelp>/TIPO_GESTIONE_1', length:2000 }" selectedKeys="{sFilter>/Finalita}">
                                        <core:ListItem key="{sHelp>TipoGestione1}" text="{sHelp>TipoGestione1}" additionalText="{sHelp>Divisione} - {sHelp>DesTipoGest1}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="grupControllo" visible="true" groupName="__$INTERNAL$" name="gc" label="{i18n>GrupControllo}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox showSecondaryValues="true" items="{ path: 'sHelp>/TIPO_GESTIONE_2', length:2000 }" selectedKeys="{sFilter>/GrupControllo}">
                                        <core:ListItem key="{sHelp>TipoGestione2}" text="{sHelp>TipoGestione2}" additionalText="{sHelp>Divisione} - {sHelp>DesTipoGest2}"/>
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem id="collective" visible="true" groupName="__$INTERNAL$" name="col" label="{i18n>Collective}" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="{sFilter>/Collective}">
                                        <items>
                                            <core:ListItem xmlns:core="sap.ui.core" key="" text=""/>
                                            <core:ListItem xmlns:core="sap.ui.core" key="X" text="X"/>
                                        </items>
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>

                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <ScrollContainer width="100%" height="100%" vertical="true" horizontal="true">
                <Table id="tbManutenzione" items="{path: 'mManutenzione>/'}" mode="MultiSelect" sticky="HeaderToolbar,ColumnHeaders">
                    <headerToolbar>
                        <OverflowToolbar style="Clear" class="sapUiMediumMarginTop">
                            <Button icon="sap-icon://history" type="Ghost" text="{i18n>ImpostaData}" id="btnImpostaData" press="onImpostaData" tooltip="{i18n>ImpostaDataTooltip}"/>
                            <Button icon="sap-icon://delete" type="Ghost" text="{i18n>CancellaData}" id="btnCancellaData" press="onCancellaData" tooltip="{i18n>CancellaDataTooltip}"/>
                            <Button icon="sap-icon://create" type="Emphasized" text="{i18n>CreaODM}" id="btnCreaODM" press="onCreaODM" tooltip="{i18n>CreaODMTooltip}"/>
                            <ToolbarSpacer/>
                            <Button icon="sap-icon://flag" type="Accept" text="{i18n>RilascioODM}" id="btnRilascioODM" press="onReleaseODM" tooltip="{i18n>RilascioODMTooltip}"/>
                            <Button icon="sap-icon://print" type="Ghost" text="{i18n>StampaDoc}" id="btnStampaDoc" press="onStampaDoc" tooltip="{i18n>StampaDocTooltip}"/>
                            <Button icon="sap-icon://action-settings" type="Ghost" text="{i18n>Consuntivazione}" id="btnConsuntivazione" press="onConsuntivazione" tooltip="{i18n>ConsuntivazioneTooltip}"/>
                            <Button icon="sap-icon://add" type="Ghost" text="{i18n>Create}" id="btnCreate" press="onCreate" tooltip="{i18n>CreateTooltip}"/>
                            <Button icon="sap-icon://connected" type="Reject" text="{i18n>Aggrega}" id="btnAggrega" press="onAggrega" tooltip="{i18n>AggregaTooltip}"/>
                            <Button icon="sap-icon://disconnected" type="Reject" text="{i18n>Disaggrega}" id="btnDisaggrega" press="onDisaggrega" tooltip="{i18n>DisaggregaTooltip}"/>
                            <Button icon="sap-icon://paper-plane" type="Emphasized" text="{i18n>SendGEOCALL}" id="btnSendGEOCALL" press="onSendGEOCALL" tooltip="{i18n>SendGEOCALLTooltip}"/>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column width="10em" id="col1" app:property="Napp"><Text text="{i18n>Napp}"/></Column>
                        <Column width="10em" id="col3" app:property="IndexPmo"><Text text="{i18n>IndexPmo}"/></Column>
